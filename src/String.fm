// Converts a String to Bits
String.to_bits(str: String): Bits
  case str {
    nil : Bits.nil,
    cons: case U16.eql(str.head, '1') {
      true : Bits.1(String.to_bits(str.tail)),
      false: Bits.0(String.to_bits(str.tail)),
    }
  }

  